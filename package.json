{
  "name": "zabaldu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run-script toggledev && dotenv -e .env.development -- npx next dev",
    "build": "npm run-script toggleprod && dotenv -e .env -- npx next build",
    "start": "next start",
    "lint": "next lint",
    "test": "dotenv -e .env.test npm run-script e2e",
    "cypress:run": "npx cypress run",
    "updatedbdev": "dotenv -e .env.development npx prisma db push",
    "updatedbprod": "dotenv -e .env npx prisma db push",
    "toggledev": "sed -i 's/cockroachdb/sqlite/g' prisma/schema.prisma",
    "toggleprod": "sed -i 's/sqlite/cockroachdb/g' prisma/schema.prisma",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "e2e:open": "start-server-and-test dev http://localhost:3000 cypress:open"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/font": "13.2.1",
    "@prisma/client": "4.10.1",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "axios": "^1.3.4",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "formik": "^2.2.9",
    "i18next": "^22.4.10",
    "moment": "^2.29.4",
    "next": "13.2.1",
    "next-auth": "^4.19.2",
    "next-i18next": "^13.1.6",
    "prisma": "^4.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^12.2.0",
    "sanitize-html": "^2.10.0",
    "sass": "^1.60.0",
    "styled-components": "^5.3.9",
    "typescript": "4.9.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.28",
    "@types/sanitize-html": "^2.9.0",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "5.14.5",
    "cypress": "^12.8.1",
    "dotenv-cli": "^7.2.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "next-router-mock": "^0.9.3",
    "start-server-and-test": "^2.0.0"
  }
}
